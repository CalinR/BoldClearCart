{
  "name": "BoldApps Clear Cart",
  "description": "Clears the Shopify Cart",
  "icons": {
    "16": "logo.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
